generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int      @id @default(autoincrement())
  name     String @unique
  password String
} 

model Study {
  id         Int     @id @default(autoincrement())
  subject    String
  study_time Int
  user_id    String
}